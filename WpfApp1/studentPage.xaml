<Window x:Class="WpfApp1.studentPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="studentPage" Height="450" Width="800">
    <Grid Background="#FF113E35">
        <StackPanel Grid.Column="0">
            <DataGrid x:Name="StudentsPageGrid" IsReadOnly="True" Grid.Row="1" Margin="10,20,10,20" Height="174" Width="668"/>
            <ComboBox x:Name="StudentsPageCombo" Width="665" Margin="30" Foreground="#FF113E35" SelectionChanged="StudentsPageCombo_SelectionChanged_1"/>
        </StackPanel>
    </Grid>
</Window>
